<main class="createCharacter">
         <h2 class="red"><span class="highlight">Create a New Student</span></h2>
    <form class="white" action="/students" method="POST">
            name:<input type="text" name="name" placeholder="name"/><br>
			Gender:<select name="Gender">
                  <option value="unknown">Unknown</option>
                 <option value="Male">male</option>
                 <option value="Female">Female</option>
            </select><br>
            class:<input type="text" name="class" placeholder="class"/><br>
			school:<input type="text" name="school" placeholder="school"/><br>
